# Ollama Configuration
OLLAMA_API_URL=http://localhost:11434/v1
OLLAMA_MODEL=llama3.2:3b

# Google Configuration
# Get API key from: https://aistudio.google.com/app/apikey
GOOGLE_API_KEY=your_google_api_key_here
GOOGLE_MODEL=gemini-2.5-flash

# OpenAI Configuration
# Get API key from: https://platform.openai.com/api-keys
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_MODEL=gpt-4.1-mini

# Agent Configuration
AGENT_NAME=PersonalFinancialAIAgent

# Enable Docker Compose Bake (optimized builds)
COMPOSE_BAKE=true

LOG_LEVEL=INFO

# RAG Configuration (Asset Retriever)
RAG_DATA_DIR=dataset/ETFs
RAG_CACHE_DIR=dataset/ETFs/.cache
RAG_EMBEDDINGS_CACHE=dataset/ETFs/.cache/embeddings.pkl
RAG_CHUNK_SIZE=800
RAG_CHUNK_OVERLAP=120
RAG_EMBEDDING_MODEL=all-roberta-large-v1

# Monte Carlo Simulation Configuration
MONTECARLO_SIMULATION_SCENARIOS=1000
MONTECARLO_SIMULATION_YEARS=20
MONTECARLO_MIN_ASSET_VOLATILITY=0.1
MONTECARLO_DEFAULT_INITIAL_INVESTMENT=1000
MONTECARLO_DEFAULT_MONTHLY_CONTRIBUTION=100
